@model HealthInsurenceSystem.Models.Person

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Added Policy List</h2>
        </div>
    </div>

    <br /><br />

    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>
                    Agent Name
                </th>
                <th>
                    Agent Number
                </th>
                <th>
                    Agent Email address
                </th>
                <th>
                    Agent Password
                </th>

                <th>
                    Agent Birth Date
                </th>

            </tr>
        </thead>
        <tbody>

            <tr>
                <td width="50%">
                     
                    @TempData["nam"]
                </td>
               
                <td width="50%">@TempData["cnm"]</td>
                <td width="50%">@TempData["em"]</td>
                <td width="30%">@TempData["pass"]</td>
                <td width="30%">@TempData["dob"]</td>


            </tr>

        </tbody>
    </table>
    <p class="lead">
        <a asp-controller="Home" asp-action="Index" class="btn btn-primary btn-sm">Go to Home</a>
    </p>

</div>